      '%47%45%54 / HTTP/1.0',
      'GET %2f HTTP/1.0',
      'GET %2F HTTP/1.0',
      'GET%20/ HTTP/1.0',
      'GET / FTP/1.0',
      'GET \ HTTP/1.0',
      'GET C:\ HTTP/1.0',
      'HTTP/1.0 / GET',
      'ALL YOUR BASE ARE BELONG TO US', 
      'GET "/" HTTP/1.0',
      "GET '/' HTTP/1.0",
      'GET `/` HTTP/1.0', 
