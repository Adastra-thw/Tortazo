<script type="text/javascript">
;(function() {
var url = window.location.toString();

if (window.history.replaceState) {
  if (false) {
    var pattern = /articles\/en_US\/kb\/.*$/;
    if (!url.match(pattern)) {
      var articleUrl = '/articles/en_US/kb/';
      if (window.location.hash) articleUrl += window.location.hash;
      window.history.replaceState(articleUrl, '', articleUrl);
    }
  } else if (/app\/ask(.*)$/.test(url) || /app\/home(.*)$/.test(url)) {
    window.history.replaceState('', '', '/');
  } else if (/ci\/pta\/login\/redirect\/home(.*)$/.test(url) || /ci\/pta\/login\/redirect\/ask(.*)$/.test(url)) {
    window.history.replaceState('', '', '/');
  } else if (/sm_hc_contact_us(.*)$/.test(url)) {
    window.history.replaceState({}, 'contact-us', url.replace(/(sm_hc_contact_us)(.*)$/, "contact-us$2"));
  }
}
}());
</script>
