function checkEmail(emailAddr) {
	
	var i;
	
	i = emailAddr.indexOf("@");
	if (i == -1) {
		return false;
	}
	
	var username = emailAddr.substring(0, i);
	var domain = emailAddr.substring(i + 1, emailAddr.length)

	i = 0;
	while ((username.substring(i, i + 1) == " ") && (i < username.length)) {
		i++;
	}
	if (i > 0) {
		username = username.substring(i, username.length);
	}

	i = domain.length - 1;
	while ((domain.substring(i, i + 1) == " ") && (i >= 0)) {
		i--;
	}
	if (i < (domain.length - 1)) {
		domain = domain.substring(0, i + 1);
	}

	if ((username == "") || (domain == "")) {
		return false;
	}
	
	var ch;
	for (i = 0; i < username.length; i++) {
		ch = (username.substring(i, i + 1)).toLowerCase();
		if (!(((ch >= "a") && (ch <= "z")) || 
			((ch >= "0") && (ch <= "9")) ||
			(ch == "_") || (ch == "-") || (ch == "."))) {
				return false;
		}
	}
	
	for (i = 0; i < domain.length; i++) {
		ch = (domain.substring(i, i + 1)).toLowerCase();
		if (!(((ch >= "a") && (ch <= "z")) || 
			((ch >= "0") && (ch <= "9")) ||
			(ch == "_") || (ch == "-") || (ch == "."))) {
				return false;
		}
	}

var aSuffix = new Array("ac","ad","ae","aero","af","ag","ai","al","am","an","ao","aq","ar","arpa","as","at","au","aw","az","ba","bb","bd","be","bf","bg","bh","bi","biz","bj","bm","bn","bo","br","bs","bt","bv","bw","by","bz","ca","cc","cf","cg","ch","ci","ck","cl","cm","cn","co","com","coop","cr","cs","cu","cv","cx","cy","cz","de","dj","dk","dm","do","dz","ec","edu","ee","eg","eh","er","es","et","eu","fi","firm","fj","fk","fm","fo","fr","fx","ga","gb","gd","ge","gf","gh","gi","gl","gm","gn","gov","gp","gq","gr","gs","gt","gu","gy","hk","hm","hn","hr","ht","hu","id","ie","il","in","info","int","io","iq","ir","is","it","jm","jo","jobs","jp","ke","kg","kh","ki","km","kn","kp","kr","kw","ky","kz","la","lb","lc","li","lk","lr","ls","lt","lu","lv","ly","ma","mc","md","mg","mh","mil","mk","ml","mm","mn","mo","mp","mq","mr","ms","mt","mu","museum","mv","mw","mx","my","mz","na","name","nato","nc","ne","net","nf","ng","ni","nl","no","nom","np","nr","nt","nu","nz","om","org","pa","pe","pf","pg","ph","pk","pl","pm","pn","pr","pro","pt","pw","py","qa","re","ro","ru","rw","sa","sb","sc","sd","se","sg","sh","si","sj","sk","sl","sm","sn","so","sr","st","store","su","sv","sy","sz","tc","td","tf","tg","th","tj","tk","tm","tn","to","tp","tr","travel","tt","tv","tw","tz","ua","ug","uk","um","us","uy","va","vc","ve","vg","vi","vn","vu","web","wf","ws","ye","yt","yu","za","zm","zr","zw");
	var bFoundSuffix = false;
	i = 0;
	while (i < aSuffix.length) {
		if (("." + aSuffix[i]) == domain.substring(domain.length - aSuffix[i].length - 1, domain.length)) {
			return true;
		}
		i++;
	}

	return false;
	
}
function validate() {
	// Which product selected?
	var option = 0;
	for (var i = 0; i < document.orderform.PROD.length; i++) {
	  if (document.orderform.PROD[i].checked) {
	    option = ++i;
	    break;
	  }
	}

	if (document.orderform.SERIAL1.value == "") {
		alert("Please enter your serial number.");
		return false;
	}
	if(option == 0){
		alert("Please select a product.");
		return false;
	}
	if (document.orderform1.prenom.value == "") {
		alert("Please enter your firstname.");
		return false;
	}
	if (document.orderform1.nom.value == "") {
		alert("Please enter your lastname.");
		return false;
	}
	if (document.orderform1.ap.value == "") {
		alert("Please enter your street.");
		return false;
	}
	if (document.orderform1.cp.value == "") {
		alert("Please enter your zip code.");
		return false;
	}
	if (document.orderform1.ville.value == "") {
		alert("Please enter your city/town.");
		return false;
	}
	if (document.orderform1.STATE.value == "") {
		alert("Please enter your state.");
		return false;
	}
	if (document.orderform1.pays.value == "Select_country") {
		alert("Please enter your country.");
		return false;
	}
	if (document.orderform1.tel.value == "") {
		alert("Please enter your phone number.");
		return false;
	}	
	if (document.orderform1.email.value == "") {
		alert("Please enter your e.mail address.");
		return false;
	}
	emailAddr=document.orderform1.email.value;
	if (!(checkEmail(emailAddr))) {
		alert("Invalid e-mail address.");
		return false;
	}
	return true;
}

function calc()
{
  // Reset submitted form values.
  document.orderform1.PROD.value = "";
   //document.orderform1.UPSELL1.value = "";
   document.orderform1.UPSELL2.value = "";
  document.orderform1.SERIAL.value = "";
  document.orderform1.DOMAINS.value = "";
  document.orderform1.downloadurl.value = "";
  document.orderform1.montant.value = 0;


  // Which product selected?
  var option = 0;
  for (var i = 0; i < document.orderform.PROD.length; i++) {
    if (document.orderform.PROD[i].checked) {
      option = ++i;
      break;
    }
  }

  // Calculate submitted values.
  var val = 0;
  switch (option) {
    case 1:
      val += 14995;
      document.orderform1.PROD.value = "SubmitWolf v8.0 Upgrade";
      document.orderform1.SERIAL.value = document.orderform.SERIAL1.value;
      document.orderform1.downloadurl.value = "http://www.trellian.com/seotoolkit/bin/SubmitWolf8.exe";
      document.orderform1.DOMAINS.value = "en:99.95";
      break;
    case 2:
      val += 24900;
      document.orderform1.PROD.value = "SEO Toolkit v3.0 Upgrade";
      document.orderform1.SERIAL.value = document.orderform.SERIAL1.value;
      document.orderform1.downloadurl.value = "http://www.trellian.com/seotoolkit/bin/SEOToolkit3.exe";
      document.orderform1.DOMAINS.value = "en:149";
      break;

    case 3:
      val += 44500;
      document.orderform1.PROD.value = "SEO Toolkit Enterprise v3.0 Upgrade";
      document.orderform1.SERIAL.value = document.orderform.SERIAL1.value;
      document.orderform1.downloadurl.value = "http://www.trellian.com/seotoolkit/bin/SEOToolkit3.exe";
      document.orderform1.DOMAINS.value = "en:295";
      break;

    case 4:
      val += 19900;
      document.orderform1.PROD.value = "SEO Toolkit v3.0 Upgrade";
      document.orderform1.SERIAL.value = document.orderform.SERIAL1.value;
      document.orderform1.downloadurl.value = "http://www.trellian.com/seotoolkit/bin/SEOToolkit3.exe";
      document.orderform1.DOMAINS.value = "en:149";
      break;

    case 5:
      val += 39500;
      document.orderform1.PROD.value = "SEO Toolkit Enterprise v3.0 Upgrade";
      document.orderform1.SERIAL.value = document.orderform.SERIAL1.value;
      document.orderform1.downloadurl.value = "http://www.trellian.com/seotoolkit/bin/SEOToolkit3.exe";
      document.orderform1.DOMAINS.value = "en:295";
      break;
  }

	
	//if (document.orderform.UPSELL1.checked) {
	//	val += 3000;
	//	document.orderform1.UPSELL1.value = "Box";
	//}


  // Display total price.
  if (document.orderform1.montant) {
    val *= parseInt(document.orderform1.LICENSES.value);
    if (val != 0) val = ((val-(val % 100)) / 100)+((val % 100 == 0) ? '.00' : '.'+(val % 100));
    document.orderform1.montant.value = val;
  }


}



