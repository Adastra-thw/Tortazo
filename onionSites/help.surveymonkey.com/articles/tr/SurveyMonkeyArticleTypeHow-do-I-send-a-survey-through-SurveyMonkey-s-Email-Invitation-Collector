if (window.history.replaceState) {
  if (true) {
    var pattern = /articles\/tr\/kb\/.*$/;
    if (!url.match(pattern)) {
      var articleUrl = '/articles/tr/kb/How-do-I-send-a-survey-through-SurveyMonkey-s-Email-Invitation-Collector';
      if (window.location.hash) articleUrl += window.location.hash;
      window.history.replaceState(articleUrl, 'Bir anketi SurveyMonkey E-posta Daveti Toplayıcısı yoluyla nasıl gönderirim?', articleUrl);
    }
  } else if (/app\/ask(.*)$/.test(url) || /app\/home(.*)$/.test(url)) {
    window.history.replaceState('', '', '/');
  } else if (/ci\/pta\/login\/redirect\/home(.*)$/.test(url) || /ci\/pta\/login\/redirect\/ask(.*)$/.test(url)) {
    window.history.replaceState('', '', '/');
