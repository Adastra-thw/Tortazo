    is_secure = false,
    url = (is_secure) ? alias_secure : alias_normal;

  url = url.replace("REPLACEME", newTrkInfo);

  if (bcookie) {
    if(window.$ && $.ajax){
      $.ajax(url);
    } else {
      YAHOO.util.Connect.asyncRequest( 'get', url, {} );
    }
  }

})();
/* ]]> */</script>
