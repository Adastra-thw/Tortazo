<script type="text/javascript">/* <![CDATA[ */

(function() {
  "use strict";

  var bcookie = LI.readCookie("bcookie"),
      urlTemplate = "/lite/page-track?ptype=PAGETYPE_PLACEHOLDER&pkey=PAGEKEY_PLACEHOLDER&jsonInfo=TRACKINGINFO_PLACEHOLDER&trk=TRK_PLACEHOLDER";


  window.ProfilePageTracking = function(pageType, pageKey, trk, jsonTrackingInfo) {
    var url, jsonTrackingInfoStr;

    if (!bcookie || !pageType || !pageKey) { 
      return; 
    }

    if (!JSON || !JSON.stringify) {
      return;
    }

    jsonTrackingInfoStr = jsonTrackingInfo && JSON && JSON.stringify 
                        ? JSON.stringify(jsonTrackingInfo)
                        : "";

    url = urlTemplate.replace("PAGETYPE_PLACEHOLDER",     pageType)
                     .replace("PAGEKEY_PLACEHOLDER",      pageKey)
                     .replace("TRK_PLACEHOLDER", trk || "")
                     .replace("TRACKINGINFO_PLACEHOLDER", jsonTrackingInfoStr);

    YConn.asyncRequest("get", url, {});
  };

})();

/* ]]> */</script>
<script type="text/javascript">/* <![CDATA[ */
(function() {

  var bcookie = escape( LI.readCookie("bcookie") ),
    newTrkInfo = '22055493,' + document.referrer.substr(0,128),
    alias_secure = 'https://www.linkedin.com/analytics/noauthtracker?type=leo%2EpageTracking&pageType=full_page&pageKey=nprofile_v2_public_fs_jsbeacon&trkInfo=REPLACEME',
    alias_normal = '/analytics/noauthtracker?type=leo%2EpageTracking&pageType=full_page&pageKey=nprofile_v2_public_fs_jsbeacon&trkInfo=REPLACEME',
    is_secure = false,
    url = (is_secure) ? alias_secure : alias_normal;

  url = url.replace("REPLACEME", newTrkInfo);

  if (bcookie) {
    if(window.$ && $.ajax){
      $.ajax(url);
    } else {
      YAHOO.util.Connect.asyncRequest( 'get', url, {} );
    }
  }

})();
