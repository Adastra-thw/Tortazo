    function dep_check(form) {
      if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Support') {		
	  form.BUDGET.selectedIndex  = 0;		
        sform0.style.display = "none";
        sform.style.display = "block";
        sform2.style.display = "block";
        sform3.style.display = "block";
        sform4.style.display = "none";
        sform5.style.display = "none";
		sformUser.style.display="";
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Advertising on Trellian') {	   
        form.BUDGET.selectedIndex  = 1; 		
//	sform0.style.display = "block";
        sform.style.display = "none";
        sform2.style.display = "none";
        sform3.style.display = "block";
        sform4.style.display = "none";
        sform5.style.display = "none";
		sformUser.style.display="";
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Sales') {	    
	    form.BUDGET.selectedIndex  = 0;		
        sform0.style.display = "none";
        sform.style.display = "block";
        sform2.style.display = "none";
        sform3.style.display = "block";
        sform4.style.display = "none";
        sform5.style.display = "none";
		sformUser.style.display="";
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Employment') {
	    form.BUDGET.selectedIndex  = 0;		
        sform0.style.display = "none";
        sform.style.display = "none";
        sform2.style.display = "none";
        sform3.style.display = "none";
        sform4.style.display = "block";
        sform5.style.display = "none";
		sformUser.style.display="none";
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'PrioritySubmit') {
	    form.BUDGET.selectedIndex  = 0;		
        sform0.style.display = "none";
        sform.style.display = "none";
        sform2.style.display = "none";
        sform3.style.display = "block";
        sform4.style.display = "none";
        sform5.style.display = "block";
      } 
	  else {	   
	  form.BUDGET.selectedIndex  = 0;
        sform0.style.display = "none";
        sform.style.display = "none";
        sform2.style.display = "none";
        sform3.style.display = "block";
        sform4.style.display = "none";
        sform5.style.display = "none";
		sformUser.style.display="";
      }

	  emails = new Array();
   
	  emails["EMAIL (SENDTO: Advertising on Trellian)"] = "contact*trellian.com";
      emails["EMAIL (SENDTO: Employment)"] = "work*trellian.com";
      emails["EMAIL (SENDTO: Feedback)"] = "feedback*trellian.com";
      emails["EMAIL (SENDTO: General Enquiry)"] = "sales*trellian.com";
      emails["EMAIL (SENDTO: Partner)"] = "partner*trellian.com";
      emails["EMAIL (SENDTO: PrioritySubmit)"] = "sales*trellian.com";
      emails["EMAIL (SENDTO: Sales)"] = "sales*trellian.com";
      emails["EMAIL (SENDTO: Support)"] = "support*trellian.com";

	  var note = document.getElementById("note");
      note.style.display = form.SENDTO.options[form.SENDTO.selectedIndex].value == 'nil' ? "none" : "block";
      if(form.SENDTO.options[form.SENDTO.selectedIndex].value != 'nil') {
        hiddenInputName = "EMAIL (SENDTO: " + form.SENDTO.options[form.SENDTO.selectedIndex].value + ")";
        emailDestination = form.emailDestination;
        emailDestination.value = emails[hiddenInputName].replace(/\*/g, "@");
      }
      form.SUBJECT.value = 'Trellian Contact Form - ' + form.SENDTO.options[form.SENDTO.selectedIndex].value;
	  form.PROD.value = 'Trellian - ' + form.SENDTO.options[form.SENDTO.selectedIndex].value;
    }
  function validate_captcha(form)
  {
			$.ajax(
				{
				  type: 'POST', 
				  url: "contact_captcha_valid.php",
				  data: $("#sendform").serialize(),
				  dataType: 'text',
				  error: function(request,error)
				  {
				      validate_captcha();
				      return false;
				  },
				  cache:false,				
				  success:function(msg)
				  {
				    var val= document.getElementById("captcha_validation").value;
				    if(msg.length==0)
				    {
				      if(val=="done")
				      {
					document.sendform.action="http://www.trellian.com/cgi-bin/webmail";
					document.sendform.submit();
				      }
				      return false;
				    }
				    else
				    {
				      document.getElementById('errorName').value = document.sendform.NAME.value;
				      document.getElementById('erroremail').value = document.sendform.EMAIL.value;
				      document.getElementById('errorusername').value = document.sendform.USERNAME.value;
				      document.getElementById('errorcomments').value = document.sendform.COMMENTS.value;
				      alert(msg);
				      document.frmError.submit();
				      return false;					
				    }//end if
				    return false;					
				}
				}
			);
  }// end function
    function urlCheck(form,str,reqList) {
      var message = "";
      for (i=2; i < urlCheck.arguments.length; i+=2) {
        same = urlCheck.arguments[i].split('|');
        if (same.length == 3) {
          if (form[same[1]].value != form[same[2]].value || form[same[1]].value.length < same[0]) {
            message = message + urlCheck.arguments[i+1] + "\n";
          }
        } 
        if (form[urlCheck.arguments[i]]) {
          box = form[urlCheck.arguments[i]];
          if ( (box.type == 'text' && box.value == '') 
          ||(box.type == 'checkbox' && ( ! box.checked )) 
          ||(box.type == 'select-one' && box.options[box.selectedIndex].value == 'nil') 
          ||(box.type == 'textarea' && box.value == '')) {
            message = message + urlCheck.arguments[i+1] + "\n";
          }
        }
	
      }
	  
	
	  // check email validity
      if (form.EMAIL.value != '') {
        if (form.EMAIL.value.indexOf("@") <= 0 || form.EMAIL.value.indexOf(".") <= 2) { message = message + "* The email address has a wrong format.\n"; }
	  }

      if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Support') {
        if (form.PRODUCT.value == '' ) { message = message + "* Please enter the product name.\n"; }
        if (form.OS.options[form.OS.selectedIndex].value == '') { message = message + "* Please select the O/S.\n"; }
        if (form.COMMENTS.value == '' ) { message = message + "* Please describe the error/trouble/bug in as much detail as possible.\nThe more detail you provide, the more quickly your problem will be resolved.\n"; }
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Advertising on Trellian') {	    
        //no extra fields to check
		if (form.BUDGET.options[form.BUDGET.selectedIndex].value == '') { message = message + "* Please select the Budget.\n"; }
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Sales') {
        if (form.PRODUCT.value == '' ) { message = message + "* Please enter the product name.\n"; }
        if (form.COMMENTS.value == '' ) { message = message + "* Please enter your comments or questions.\n"; }
      } else if (form.SENDTO.options[form.SENDTO.selectedIndex].value == 'Employment') {
        if (form.POSITION.value == '' ) { message = message + "* Please select the position to apply.\n"; }
        if (form.COVER_LETTER.value == '' ) { message = message + "* Please enter cover letter.\n"; }
        if (form.FILE.value == '' ) { message = message + "* Please attach your resume/CV.\nFormats accepted: doc, pdf, txt\n"; }
      } else {
        if (form.COMMENTS.value == '' ) { message = message + "* Please enter your comments or questions.\n"; }
      }      
      
      if (message != '' ) {
        alert("Error:\n" + str + message);
        return false;
      } else {
	form.captcha_validation.value="done";
	return false;
    	form.SET_EMAIL_FROM.value = form.NAME.value + " <" + form.EMAIL.value + ">\r\nReply-To:"+ form.EMAIL.value;
        sendform.submit();
    //		return true;
      }
      
    }

    function change_operating_system(theSelect) {
      xptype.style.display = (theSelect.value == "Windows XP Home" || theSelect.value == "Windows XP Professional" || theSelect.value == "Windows XP Server") ? "block" : "none";
      othertype.style.display = (theSelect.value == "Other") ? "block" : "none";
    }
  </script>
