<script language="JavaScript" type="text/javascript">
	function loading()
	{
	
	
		if(document.getElementById('url').value != "")
		{
			//document.getElementById('spnLoading').style.display = "inline" ;	
			document.getElementById('importdata').value='1';
			document.f1.action="free-submission-start.php";
			document.f1.submit();			
		}
		else
		{
			alert("Please enter your URL")
		}		
	}
	function showMetaForm()
	{
		document.getElementById('url_meta_date_form').style.display="block";	
	}
	function hideMetaForm()
	{
		document.getElementById('url_meta_date_form').style.display="none";	
	}
</script>
